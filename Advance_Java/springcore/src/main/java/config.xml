<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

   <bean class ="com.springcore.Student" name="student1">
   <property name="studentId" value = "1" />
   <property name="studentName" value="Vishal" />
   <property name="studentAdress" value = "Pune" />
   </bean>
   
   <bean class ="com.springcore.Student" name="student2">
   <property name="studentId" value = "2" />
   <property name="studentName" value="Nikhil" />
   <property name="studentAdress" value = "Nagar" />
   </bean>
   

</beans> 